{
  "ignition": {
    "config": {
      "merge": [
        {
          "compression": "gzip",
          "source": "data:;base64,"
        }
      ]
    },
    "version": "3.4.0"
  },
  "storage": {
    "filesystems": [
      {
        "device": "/dev/sda",
        "format": "ext4",
        "path": "/var/data",
        "wipeFilesystem": false
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "enabled": true,
        "name": "var-data@sda.mount"
      }
    ]
  }
}
