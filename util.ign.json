{
  "ignition": {
    "config": {
      "merge": [
        {
          "compression": "gzip",
          "source": "data:;base64,"
        }
      ]
    },
    "version": "3.4.0"
  },
  "storage": {
    "filesystems": [
      {
        "device": "/dev/sdb",
        "format": "ext4",
        "path": "/var/data",
        "wipeFilesystem": false
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "contents": "[Unit]\nDescription=Data Volume\nDefaultDependencies=no\nConflicts=umount.target\nBefore=local-fs.target umount.target\nAfter=var.mount\n\n[Mount]\nWhat=/dev/%i\nWhere=/var/data\nType=ext4\n\n[Install]\nWantedBy=local-fs.target\n",
        "enabled": false,
        "name": "var-data@.mount"
      },
      {
        "enabled": true,
        "name": "var-data@sdb.mount"
      }
    ]
  }
}
